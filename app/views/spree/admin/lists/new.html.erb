<% content_for :page_title do %>
  <%= link_to Spree.t(:index, scope: :list), spree.admin_lists_url %> /
  <%= Spree.t(:new, scope: :list) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @list } %>

<%= form_for [:admin, @list] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
